	<query language="SQL"><![CDATA[SELECT 
    g.id,
    g.date,
    g.total,
    g.paid_amount,
    g.balance,
    CONCAT(s.first_name, ' ', s.last_name) AS supplier,
    COALESCE(SUM(gi.qty), 0) AS qty
FROM grn g
LEFT JOIN supplier s ON g.supplier_id = s.id
LEFT JOIN grn_item gi ON g.id = gi.grn_id
GROUP BY 
    g.id,
    g.date,
    g.total,
    g.paid_amount,
    g.balance,
    s.first_name,
    s.last_name
ORDER BY g.id DESC;]]></query>
