	<query language="SQL"><![CDATA[SELECT 
    p.name AS product, 
    s.selling_price AS unit_price, 
    ii.qty, 
    (s.selling_price * ii.qty) AS item_total
FROM invoice_item ii
JOIN stock s ON ii.stock_id = s.id
JOIN product p ON s.product_code = p.code
WHERE ii.invoice_id = $P{invoice_id}]]></query>
